{
    "service": {
        "name": "nginx",
        "port": 80,
        "check": {
            "id": "nginx",
            "name": "nginx on port 80",
            "http": "http://localhost/ping",
            "interval": "10s",
            "timeout": "1s"
        }
    }
}
// Based on https://github.com/smebberson/docker-alpine/blob/master/alpine-consul-nginx/root/etc/consul/conf.d/nginx.json